{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "types": ["vite/client"],
    "lib": ["es2019", "dom", "dom.iterable", "WebWorker"],
    "jsx": "react-jsx",
    "moduleResolution": "bundler",
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"],
    },
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "downlevelIteration": true,
    "skipLibCheck": true,
  }
}